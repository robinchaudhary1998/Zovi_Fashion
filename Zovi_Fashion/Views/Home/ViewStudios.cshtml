@model IEnumerable<Zovi_Fashion.Models.Studio>

@{
    ViewData["Title"] = "ViewStudios";
}

<div class="text-center">
    <h1 class="display-4">All Studios Entry</h1>
</div>
<div class="card-columns">
    @foreach (var item in Model)
    {
        <div class="card">
            <img class="card-img-top" src="../../studiophotos/@item.StudioID@item.Extension" alt="Card image">
            <div class="card-body">
                <h5 class="card-title">@item.Title</h5>
                <a class="btn btn-warning" asp-action="StudioDetails" asp-route-id="@item.StudioID">View Studio Details</a>
            </div>
        </div>
    }
</div>